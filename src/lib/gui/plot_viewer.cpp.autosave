#include "plot_viewer.h"
#include <QLabel>
#include <QHBoxLayout>
#include <QPushButton>

plot_viewer::plot_viewer (QWidget *parent) : QWidget (parent)
{
  QHBoxLayout *hlo_main = new QHBoxLayout;
  {
    QLabel *label = new QLabel (this);
    label->setText ("I am a plot");
    hlo_main->addWidget (label);
    
    QPushButton *pb = new QPushButton (this);
    pb->setText ("Push me!");
    hlo_main->addWidget (pb);
  }
  setLayout (hlo_main);
}
